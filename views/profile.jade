extends layout

block content
  div(class="grid")
    div(class="row")
      h3="User INFO"
    div(class="row")
      button(class="bg-color-blue", id="services_list")="Services"
      button(class="bg-color-blue", id="friends_list", disabled)="Friends"
    div(class="row")
      div(class="profile_friends_container")
        table
          th="Service"
          th="Updated"
            each row in service_list
              tr
                td
                  h3=row.service
                td=row.lastupdate
      
