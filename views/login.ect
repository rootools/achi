<% extend 'layout.ect' %>

<center>
  <div id="loginForm">
    <form class="loginForm" method="POST" action="/login">
      <div class="input-control text">
        <input type="input" class="loginForm" name="loginEmail">
      </div>
      <div class="input-control password">
        <input type="password" class="loginForm" name="loginPass">
      </div>
      <input type="submit" value="Submit" id="loginFormSubmitButton">
    </form>
  </div>
</center>
