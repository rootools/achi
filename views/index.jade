extends layout

block content
  script(src="javascripts/dashboard.js")
  a(href="/logout") Logout
  p user: #{user}
  a(href="/add_service/vk") VK Add
  br
  a(href="/add_service/twitter") Twitter Add
  br
  a(href="/add_service/facebook") Facebook Add
  hr
  center
    div(id="dashboard")
      div(id="dashboard_menu")
        .dashboard_menu_button= "main"
        each item in achivList
          .dashboard_menu_button= item.service
      div(id="dashboard_list")
